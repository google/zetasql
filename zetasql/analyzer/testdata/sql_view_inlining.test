| +-TwoIntsView.b#2 AS b [INT64]
| +-TwoIntsView.a#1 AS a [INT64]
+-query=
  +-ProjectScan
    +-column_list=TwoIntsView.[b#2, a#1]
    +-input_scan=
      +-ProjectScan
        +-column_list=TwoIntsView.[a#1, b#2]
        +-expr_list=
        | +-a#1 := Literal(type=INT64, value=1)
        | +-b#2 := Literal(type=INT64, value=2)
        +-input_scan=
          +-SingleRowScan
--
ALTERNATION GROUP: a
--
