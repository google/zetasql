        |   |   +-expr_list=
        |   |   | +-$col1#3 := Literal(type=INT64, value=1)
        |   |   +-input_scan=
        |   |     +-AggregateScan
        |   |       +-column_list=[$aggregate.$agg1#4]
        |   |       +-input_scan=
        |   |       | +-FilterScan
        |   |       |   +-column_list=[KeyValue.Key#1]
        |   |       |   +-input_scan=
        |   |       |   | +-TableScan(column_list=[KeyValue.Key#1], table=KeyValue, column_index_list=[0])
        |   |       |   +-filter_expr=
        |   |       |     +-Literal(type=BOOL, value=true)
        |   |       +-aggregate_list=
        |   |         +-$agg1#4 :=
