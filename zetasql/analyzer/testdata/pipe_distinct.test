  (select 1 y, 2 a, 3 b) t1
JOIN
  (select x as y, x as z, 4 b from (select 1 x)) t2
USING (y, b)
|> STATIC_DESCRIBE
|> DISTINCT
|> STATIC_DESCRIBE
|> WHERE y = t2.{{b|y}}
