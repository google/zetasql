# Result scan with ORDER BY has is_ordered.
select 1 x
|> ORDER BY x
--
QueryStmt
+-output_column_list=
| +-$query.x#1 AS x [INT64]
+-query=
  +-OrderByScan
    +-column_list=[$query.x#1]
    +-is_ordered=TRUE
    +-input_scan=
    | +-ProjectScan
    |   +-column_list=[$query.x#1]
    |   +-expr_list=
    |   | +-x#1 := Literal(type=INT64, value=1)
    |   +-input_scan=
    |     +-SingleRowScan
    +-order_by_item_list=
      +-OrderByItem
        +-parse_location=67-68
        +-column_ref=
          +-ColumnRef(type=INT64, column=$query.x#1)
==

