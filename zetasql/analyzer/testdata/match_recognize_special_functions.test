select * from KeyValue MATCH_RECOGNIZE(
  ORDER BY key
  MEASURES
    ARRAY_AGG(DISTINCT
              {{key|match_row_number()}}
              ) AS m1

  PATTERN (a+)
  DEFINE
    A AS true
)
