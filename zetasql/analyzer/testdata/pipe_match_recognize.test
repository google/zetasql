              a AS(LENGTH(projectscan_6.a_2)) < 10
            OPTIONS(use_longest_match = true)
          )
          AS matchrecognizescan_10
      ) AS projectscan_12
    WHERE
      ((projectscan_12.a_11) + (projectscan_12.a_4)) > 10
  ) AS filterscan_13;

