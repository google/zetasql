    |     | +-FunctionCall(ZetaSQL:error(STRING) -> INT32)
    |     |   +-Literal(type=STRING, value="xxx")
    |     +-ColumnRef(type=INT32, column=SimpleTypes.int32#1)
    +-input_scan=
      +-TableScan(column_list=[SimpleTypes.int32#1], table=SimpleTypes, column_index_list=[0])
