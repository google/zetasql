# the LHS just like without it. Note that the type does not change.
select gt.* from
graph_table(aml
  match (n IS Account)
  {{|OPTIONAL }}CALL (n)
  {
    MATCH (n IS Person)
    RETURN n.age
  }
  return n.balance, age
) gt
