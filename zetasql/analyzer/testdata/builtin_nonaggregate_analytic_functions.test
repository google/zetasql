        | +-AggregateScan
        |   +-column_list=[$groupby.int64#22, $aggregate.$agg1#20]
        |   +-input_scan=
        |   | +-TableScan(column_list=SimpleTypes.[int64#2, uint32#3], table=SimpleTypes, column_index_list=[1, 2])
        |   +-group_by_list=
