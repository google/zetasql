          | +-partition_by=
          | | +-WindowPartitioning
          | |   +-partition_by_list=
          | |     +-ColumnRef(type=INT64, column=KeyValue.Key#1)
          | +-analytic_function_list=
          |   +-$analytic2#8 := AnalyticFunctionCall(ZetaSQL:row_number() -> INT64)
          +-AnalyticFunctionGroup
          | +-order_by=
          | | +-WindowOrdering
