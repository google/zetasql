    +-input_scan=
      +-AnalyticScan
        +-column_list=[KeyValue.Key#1, KeyValue.Value#2, $analytic.$analytic1#9, $analytic.$analytic2#10, $analytic.$analytic3#11, $analytic.$analytic4#12, $analytic.$analytic5#13, $analytic.$analytic6#14]
