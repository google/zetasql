        |   | |   +-field_descriptor=int32_val
        |   | |   +-default_value=77
        |   | +-int64_key_1#5 :=
        |   | | +-GetProtoField
        |   | |   +-type=INT64
        |   | |   +-expr=
        |   | |   | +-ColumnRef(type=PROTO<zetasql_test__.KitchenSinkPB>, column=TestTable.KitchenSink#3)
