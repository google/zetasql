  SAFE.ARRAY_TRANSFORM([-1, 0, 10], e -> 1/e),
  SAFE.ARRAY_FILTER([-1, 0, 10], e -> 1/e > 0),
  SAFE.ARRAY_INCLUDES([-1, 0, 10], 0),
  SAFE.ARRAY_INCLUDES([-1, 0, 10], e -> 1/e > 0),
  SAFE.ARRAY_INCLUDES_ALL([-1, 0, 10], [1, 0]),
  SAFE.ARRAY_INCLUDES_ANY([-1, 0, 10], [1, 0])
