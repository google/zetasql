# Test all array functions with SAFE, with no rewrites
[language_features=V_1_2_SAFE_FUNCTION_CALL,V_1_3_INLINE_LAMBDA_ARGUMENT,V_1_4_SAFE_FUNCTION_CALL_WITH_LAMBDA_ARGS]
[enabled_ast_rewrites=NONE]
