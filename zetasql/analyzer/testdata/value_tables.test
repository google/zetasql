        |   | +-TableScan(table=EmptyMessageValueTable, alias="e")
        |   +-filter_expr=
        |     +-Literal(type=BOOL, value=true)
        +-aggregate_list=
          +-$agg1#2 := AggregateFunctionCall(ZetaSQL:$count_star() -> INT64)
