SELECT * FROM SimpleTypes
MATCH_RECOGNIZE(
  {{PARTITION BY int64, int32|}}
  ORDER BY string
  MEASURES
    MAX(int32 + SUM(int32 + int64 + MIN(double) GROUP BY int64) GROUP BY int32) AS max_agg,
    ARRAY_AGG(string GROUP BY string ORDER BY MIN(double)) AS array_agg
  PATTERN (A B)
  DEFINE
    A AS int64 < 10,
    B AS int32 >= 10
)
