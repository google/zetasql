# subpipeline filled in just for the selected case.
FROM KeyValue
|> {{IF true|IF false}} THEN ( |> EXTEND 1 x )
   {{ELSEIF true|ELSEIF false}} THEN ( |> EXTEND 2 y )
   ELSE ( |> EXTEND 3 z )
