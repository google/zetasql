[SQLBUILDER_OUTPUT]
SELECT
  aml_1.a_6 AS id
FROM
  GRAPH_TABLE(
    aml
    MATCH
      (a_2
      IS (% | ! %))
      -[a_3
      IS (% | ! %)]->
      (a_4
      IS (% | ! %))
    LET
      a_5 = AFN_AGG(a_2.age) OVER (PARTITION BY a_2
        ORDER BY a_2.id RANGE BETWEEN UNBOUNDED PRECEDING AND CURRENT ROW)
    FILTER
      WHERE
        a_5 > 5
    RETURN
      a_2.id AS a_6
  ) AS aml_1;

[SQLBUILDER_TARGET_SYNTAX_MODE pipe]
[SQLBUILDER_OUTPUT]
FROM
  GRAPH_TABLE(
    aml
