[UNPARSED_SQL]
SELECT
  aml_1.a_6 AS len
FROM
  GRAPH_TABLE(
    aml
    MATCH
      a_2 = (a_3
      IS (% | ! %))
    LET
      a_4 = PATH(a_3)
    LET
      a_5 = a_2 || a_4
    RETURN
      PATH_LENGTH(a_5) AS a_6
  ) AS aml_1;

