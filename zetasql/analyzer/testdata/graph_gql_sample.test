SELECT
  aml_1.a_12 AS p_len,
  aml_1.a_13 AS age,
  aml_1.a_14 AS highest
FROM
  GRAPH_TABLE(
    aml
    MATCH
      a_2 = (a_3
      IS (% | ! %))
      (
        (a_4
        IS (% | ! %))
        -[a_5
        IS (% | ! %)]->
        (a_6
        IS (% | ! %))
      ) { 3, 5 }
      (a_7
      IS (% | ! %))
      -[a_8
      IS (% | ! %)]-
      (a_9
      IS (% | ! %))
    TABLESAMPLE reservoir(5 ROWS PARTITION BY a_2, a_3.age, ELEMENT_ID(a_7), SOURCE_NODE_ID(a_8)) WITH WEIGHT AS a_10 REPEATABLE (123)
    LET
      a_11 = MAX(a_5.amount)
    RETURN
      PATH_LENGTH(a_2) AS a_12,
      a_3.age AS a_13,
      a_11 AS a_14
  ) AS aml_1;

