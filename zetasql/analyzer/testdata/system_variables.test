# Simple retrieval of system variable
SELECT @@int64_system_variable;
--
QueryStmt
+-output_column_list=
| +-$query.$col1#1 AS `$col1` [INT64]
+-query=
  +-ProjectScan
    +-column_list=[$query.$col1#1]
    +-expr_list=
    | +-$col1#1 := SystemVariable(int64_system_variable, type=INT64)
    +-input_scan=
      +-SingleRowScan
