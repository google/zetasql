select * from NoColumns
--
ERROR: SELECT * would expand to zero columns [at 1:8]
select * from NoColumns
       ^
==

select nc.x from NoColumns nc
--
ERROR: Name x not found inside nc [at 1:11]
select nc.x from NoColumns nc
          ^
