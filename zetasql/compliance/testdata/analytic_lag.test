FROM (SELECT 1 row_id, [1, 2, 3] array_val UNION ALL
      SELECT 2, [2, 3, 4] UNION ALL
      SELECT 3, [3, 4, 5] UNION ALL
      SELECT 5, CAST(null as ARRAY<int64>) UNION ALL
      SELECT 6, [6, 5] UNION ALL
      SELECT 7, CAST(null as ARRAY<int64>))
--
ARRAY<STRUCT<row_id INT64, array_val ARRAY<>, ARRAY<>>>[
  {
    1,
    ARRAY<INT64>[1, 2, 3],
    ARRAY<INT64>[-1, -1, -1]
  },
  {
    2,
    ARRAY<INT64>[2, 3, 4],
    ARRAY<INT64>[-1, -1, -1]
  },
  {
    3,
    ARRAY<INT64>[3, 4, 5],
    ARRAY<INT64>[1, 2, 3]
  },
  {
    5,
    ARRAY<INT64>(NULL),
    ARRAY<INT64>[2, 3, 4]
  },
  {
    6,
    ARRAY<INT64>[6, 5],
    ARRAY<INT64>[3, 4, 5]
  },
  {7, ARRAY<INT64>(NULL), ARRAY<INT64>(NULL)}
]
==

[name=lag_partitionby]
