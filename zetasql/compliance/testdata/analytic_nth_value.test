  {NULL, 1, 1, 1},
  {false, 9, 9, 9},
  {false, 7, 9, 9},
  {false, 5, 9, 9},
  {false, 3, 7, 7},
  {true, 10, 10, 10},
  {true, 8, 10, 10},
  {true, 6, 10, 10},
  {true, 4, 8, 8},
  {true, 2, 6, 6}
]
