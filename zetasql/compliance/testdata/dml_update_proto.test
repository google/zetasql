       cast('''repeated_recursive_pb {
                 repeated_recursive_pb {}
                 repeated_recursive_pb {
                   repeated_recursive_pb {}
                   repeated_recursive_pb {}
                   repeated_recursive_pb {
                     repeated_recursive_pb {}
                     repeated_recursive_pb {}
                     repeated_recursive_pb {}
                     repeated_recursive_pb {
                       int64_val: 100
                     }
                   }
                 }
               }''' as zetasql_test__.RecursivePB) as proto_value
