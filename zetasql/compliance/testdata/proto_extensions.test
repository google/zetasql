[prepare_database]
CREATE TABLE Extensions AS
SELECT 1 key, cast('''
  int64_key_1: 1
  int64_key_2: 1
''' as `zetasql_test.KitchenSinkPB`) as value
union all
select 2, null
union all
select 4, cast('''
  int64_key_1: 4
  int64_key_2: 4

  [zetasql_test.KitchenSinkExtension.optional_extension] { value: "504" }
  [zetasql_test.KitchenSinkExtension.int_extension]: 506
  [zetasql_test.KitchenSinkExtension.repeated_extension] { value: "507" }
  [zetasql_test.KitchenSinkExtension.repeated_extension] { value: "508" }
''' as `zetasql_test.KitchenSinkPB`) as value
