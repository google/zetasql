    SELECT 3 AS x
  )
), T2 AS (
  SELECT ARRAY_AGG(y) AS unordered_arr2
  FROM (
    SELECT "hello" AS y
    UNION ALL
    SELECT "world" AS y
  )
)
SELECT COUNT(*)
FROM T1, T2, UNNEST(
  T1.unordered_arr1,
  T2.unordered_arr2 AS unordered_arr2)
--
ARRAY<STRUCT<INT64>>[{3}]

NOTE: Reference implementation reports non-determinism.
==

