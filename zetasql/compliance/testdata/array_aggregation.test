   SELECT "b" UNION ALL
   SELECT "c")
--
ARRAY<STRUCT<c ARRAY<>>>[
  {ARRAY<STRUCT<INT64, STRING>>[
     {2, "a"},
     {1, "b"},
     {1, "c"},
     {3, "a"},
     {2, "b"},
     {2, "c"},
     {1, "a"},
     {3, "b"},
     {3, "c"}
   ]}
]
==
