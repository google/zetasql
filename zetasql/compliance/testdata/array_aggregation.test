[name=array_agg_BOOL_basic]
SELECT ARRAY_AGG(elem) FROM (
  SELECT true AS elem UNION ALL
  SELECT false UNION ALL
  SELECT true)
--
ARRAY<STRUCT<ARRAY<>>>[{ARRAY<BOOL>[unknown order:false, true, true]}]
