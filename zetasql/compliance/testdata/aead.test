  kt_id,
  pt_id,
  plaintext_string,
  plaintext_bytes,
  AEAD.DECRYPT_STRING(
    keyset,
    AEAD.ENCRYPT(keyset, plaintext_string, aad_string),
    aad_string),
  AEAD.DECRYPT_BYTES(
    keyset,
    AEAD.ENCRYPT(keyset, plaintext_bytes, aad_bytes),
