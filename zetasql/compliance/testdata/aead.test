  SELECT AEAD.ENCRYPT(keyset, plaintext_string, aad_string) AS ciphertext
  FROM KeysetTable
  CROSS JOIN PlaintextTable
  WHERE kt_id = 1 AND pt_id = 1
)
SELECT ciphertext = ciphertext
FROM T
