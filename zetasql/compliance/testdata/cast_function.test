# execution.
[name=cast_hex_string_integers_from_subquery]
SELECT CAST(A AS INT32),
       CAST(B AS INT32),
       CAST(C AS INT32),
       CAST(D AS INT32),
       CAST(E AS UINT32),
       CAST(F AS UINT32),
       CAST(G AS INT64),
       CAST(H AS INT64),
       CAST(I AS UINT64),
       CAST(J AS UINT64),
       CAST(K AS UINT64),
       CAST(L AS UINT64)
