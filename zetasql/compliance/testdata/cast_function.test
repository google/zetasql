# Set this in subqueryies to keep literals from being converted prior to
# execution.
[name=cast_hex_string_integers_from_subquery]
SELECT CAST(A AS INT32),
       CAST(B AS INT32),
       CAST(C AS INT32),
       CAST(D AS INT32),
       CAST(E AS UINT32),
       CAST(F AS UINT32),
       CAST(G AS INT64),
       CAST(H AS INT64),
       CAST(I AS UINT64),
       CAST(J AS UINT64),
       CAST(K AS UINT64),
       CAST(L AS UINT64)
FROM (
    SELECT '0x121' AS A,
           '-0x121' AS B,
           '+0x121' AS C,
           '   +0x121   ' AS D,
           '0x121' AS E,
           '0xFFFFFFFF' AS F,
           '0x121abc' AS G,
           '-0x121abc' AS H,
           '0x121abc' AS I,
           '0X121abc' AS J,
           '0x121ABCDEF' AS K,
           '0xFFFFFFFFFFFFFFFF' AS L
)
--
ARRAY<STRUCT<INT32,
             INT32,
             INT32,
             INT32,
             UINT32,
             UINT32,
             INT64,
             INT64,
             UINT64,
             UINT64,
             UINT64,
             UINT64>>
[{289,
  -289,
  289,
  289,
  289,
  4294967295,
  1186492,
  -1186492,
  1186492,
  1186492,
  4859874799,
  18446744073709551615}]
