ModuleStatement [0-28]
  PathExpression [7-14]
    Identifier(foo) [7-10]
    Identifier(bar) [11-14]
  OptionsList [23-28]
    OptionsEntry [24-27]
      Identifier(a) [24-25]
      IntLiteral(1) [26-27]
